{
  "$schema": "https://mintlify.com/schema.json",
    "api": {
      "baseUrl": "https://api.documind.xyz", 
      "auth": {
        "method": "bearer" 
      },
    "playground": {
      "mode": "show",
      "disableProxy": true
    },
    "paramFields": {
      "expanded": "topLevel"
    }
  },
  "name": "Documind",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "layout": "topnav",
  "modeToggle": {
    "default": "dark"
  },
  "sidebar": {
    "items": "card"
  },
  "favicon": "/favicon.svg",
  "font":{
    "headings": {
      "family": "Roobert",
      "weight": 800,
      "url": "https://framerusercontent.com/assets/Ue46Yo2GHYihMTKb21t9dajUI.woff2",
      "format": "woff2"
    },
    "body": {
      "family": "Inter"
    }
  },
  "colors": {
    "primary": "#1098F7",
    "light": "#1098F7",
    "dark": "#1098F7",
    "background" : {
      "light": "#f2f2f2",
      "dark": "#212121"
    },
    "anchors": {
      "from": "#1098F7",
      "to": "#7AC2FB"
    }
  },
  "topbarLinks": [
    {
      "name": "Try it out",
      "url": "https://www.documind.xyz/playground"
    }
  ],
